<h1 class="all-drinks-title">RESULTS</h1>
<div class="all-drinks-section">
  {{#each searchQuery}}
    <a href="/drink/{{this._id}}">
      <div class="all-drinks">
        <h1>{{this.name}}</h1>
        {{#if this.glass}}
          <p>#{{this.glass}}</p>
        {{/if}}
      </div>
    </a>
  {{/each}}
</div>
